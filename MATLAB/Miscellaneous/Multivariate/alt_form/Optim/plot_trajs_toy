traj_0 = reshape(x_0(1:10),2,5)
traj = reshape(x(1:10),2,5)

figure;
plot(optParams.P_feas,'color','green');
hold on;
plot(optParams.P1);
hold on;grid on;
plot(optParams.P_final,'color','gray');
for i = 1:5
    hold on;
    plot(traj(1,i),traj(2,i),'k*');
    hold on
    plot(traj_0(1,i),traj(2,i),'bo');
end